{
  "name": "webp-converter",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/web",
    "packages/cli"
  ],
  "scripts": {
    "dev": "npm run dev -w @webp-converter/web",
    "build": "npm run build -w @webp-converter/web && npm run build -w webp-convert",
    "test": "npm run test --workspaces --if-present",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "deploy": "npm run deploy -w @webp-converter/web"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15"
  }
}
